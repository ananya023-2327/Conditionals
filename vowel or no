#include<iostream>
#include<string.h>
using namespace std;
int main(){
    char ch;
    string vowel = "AaEeIiOoUu";
    cout<<"Enter a character"<<endl;
    cin>>ch;
    if (vowel.find(ch) != string::npos){
        printf("The enetred character is a vowel");
    }
    else{
        printf("the entered char is a consonant");
    }
return 0;
}